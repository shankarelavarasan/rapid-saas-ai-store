# Example environment variables
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=https://hsqpadurdkdtsgosamep.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# AI Services
OPENAI_API_KEY=sk-your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Trae AI API (Open Source)
TRAE_AI_API_URL=https://api.trae.ai
TRAE_AI_API_KEY=your_trae_ai_api_key_here

# Image Generation & Storage
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# App Store APIs (Optional)
APPLE_CONNECT_API_KEY=your_apple_connect_api_key
GOOGLE_PLAY_API_KEY=your_google_play_api_key

# Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Authentication
JWT_SECRET=your_super_secure_jwt_secret_here_min_32_chars
JWT_EXPIRES_IN=7d

# App Generation Settings
APP_GENERATION_TIMEOUT=300000
MAX_CONCURRENT_GENERATIONS=5
DEFAULT_APP_CATEGORY=utility

# Revenue Split (percentage)
PLATFORM_REVENUE_SHARE=30
DEVELOPER_REVENUE_SHARE=70

# Frontend URLs
FRONTEND_URL=http://localhost:3000
API_BASE_URL=http://localhost:3000/api

# File Upload Limits
MAX_FILE_SIZE=10485760
MAX_FILES_PER_UPLOAD=10
ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,svg,pdf,zip

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Demo Mode (set to true to enable demo features)
DEMO_MODE=false
DEMO_APPS_LIMIT=5